      "optional": true
    },
    "webpack": {
      "optional": true
    }
  },
  "dependencies": {
    "autoprefixer": "^10.4.17",
    "cssnano": "^6.1.2",
    "lodash.merge": "^4.6.2",
    "postcss": "^8.4.38",
    "postcss-hexrgba": "github:thormeier/postcss-hexrgba",
    "postcss-mixins": "^9.0.4",
    "postcss-nesting": "^12.1.1",
    "postcss-url": "^10.1.3",
    "tailwindcss": "^3.4.3",
    "unplugin": "^1.5.1",
    "unplugin-icons": "^0.18.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.2.2",
    "@nuxt/kit": "^3.8.2",
    "@nuxt/schema": "^3.8.2",
    "@types/lodash.merge": "^4.6.9",
    "@types/node": "^20.10.3",
